    <icon src="resources/ios/icon/icon_152x152.png" width="152" height="152"/>
    <icon src="resources/ios/icon/icon_29x29.png" width="29" height="29"/>
    <icon src="resources/ios/icon/icon_58x58.png" width="58" height="58"/>
    <!-- <icon src="resources/ios/icon/icon-small@3x.png" width="87" height="87"/> -->
    <splash src="resources/ios/splash/iPhone5-4.png" height="1136" width="640"/>
    <splash src="resources/ios/splash/iPhone6-4.7.png" height="1334" width="750"/>
