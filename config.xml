    <icon src="resources/ios/icon/icon-60.png" width="60" height="60"/>-->
    <icon src="resources/ios/icon/icon_120x120.png" width="120" height="120"/>
    <icon src="resources/ios/icon/1806plus.png" width="180" height="180"/>
